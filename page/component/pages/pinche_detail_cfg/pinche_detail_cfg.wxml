<view class='bgwrap'>
  <map id="map" longitude="{{from_place_longitude}}" latitude="{{from_place_latitude}}" scale="14" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange"
    show-location style="width: 100%; height: 300px;"></map>
</view>
<view class="page page-form">
  <view class="weui-cells__title submit-form-top-title">
    <text>联系人信息</text>
  </view>
  <view class="weui-cells contact-form">
    <view class="weui-cell">
      <image src="../../../../page/common/images/ren.png" mode="widthFix" />
      <view class="weui-cell__hd">
        <text class="weui-label">车主</text>
      </view>
      <view class="weui-cell__bd">
        {{username}}
        <text style='color:grey;padding-left:20rpx;font-size:11px;'> ({{sex}})</text>
      </view>
      <view class="weui-cell__ft"></view>
    </view>

    <view class="weui-cell">
      <image src="../../../../page/common/images/shou.png" mode="widthFix" />
      <view class="weui-cell__hd">
        <text class="weui-label">手机号</text>
      </view>
      <view class="weui-cell__bd">
        {{userphone}}
      </view>
      <view class="weui-cell__ft"></view>
    </view>
    <view class="weui-cell">
      <image src="../../../../page/common/images/kefu.png" mode="widthFix" />
      <view class="weui-cell__hd">
        <text class="weui-label">微信号</text>
      </view>
      <view class="weui-cell__bd">
        {{car ? car : '(未填写)'}}
      </view>


      <button id="{{item.id}}" bindtap="copyTBL" style=' margin-right:80rpx;width:45px;height:30px;  float:right; border:1px solid #17a976;border-radius: 15%; background-color:grey;background:white url(data:image/png;base64,) no-repeat 14px 6px; background-size: 17px 18px;  text-align:left; text-indent: 15px;  '>
      </button>



    </view>



  </view>

  <view class="weui-cells__title">
    <text>拼车信息</text>
  </view>
  <view class="weui-cells">
    <view class="weui-cell">
      <image src="../../../../page/common/images/qi.png" mode="widthFix" />
      <view class="weui-cell__hd">
        <text class="weui-label">起点</text>
      </view>

      <view class="weui-cell__bd" style='overflow:hidden;text-overflow:ellipsis;white-space:nowrap;'>
        <text selectable='true'>  {{from_place}}</text>
      </view>
      <image data-navigation-type="1" bindtap="openNavigation" src="../../../../page/common/images/daohang.png" mode="widthFix" />

    </view>
    <view class="weui-cell">
      <image src="../../../../page/common/images/tu.png" mode="widthFix" />
      <view class="weui-cell__hd">
        <text class="weui-label">途经</text>
      </view>
      <view class="weui-cell__bd" style='overflow:hidden;text-overflow:ellipsis;white-space:nowrap;'>
        <text selectable='true'>   {{mid_place ? mid_place : '(未填写)'}}</text>
      </view>

    </view>
  </view>
  <view class="weui-cell">
    <image src="../../../../page/common/images/zhong.png" mode="widthFix" />
    <view class="weui-cell__hd">
      <text class="weui-label">终点</text>
    </view>

    <view class="weui-cell__bd" style='overflow:hidden;text-overflow:ellipsis;white-space:nowrap;'>
      <text selectable='true'>  {{to_place}}</text>
    </view>
    <image data-navigation-type="0" bindtap="openNavigation" src="../../../../page/common/images/daohang.png" mode="widthFix" />

  </view>
 
  <view class="weui-cell">
    <image src="../../../../page/common/images/time.png" mode="widthFix" />
    <view class="weui-cell__hd">
      <text class="weui-label">时间</text>
    </view>
    <view class="weui-cell__bd">
      {{date}}
      <text style='color:#FF7F00'>{{time}}</text>
    </view>
  </view>
  <view class="weui-cell">
    <image src="../../../../page/common/images/huo1.png" mode="widthFix" />
    <view class="weui-cell__hd">
      <text class="weui-label">载重</text>
    </view>
    <view class="weui-cell__bd">
   {{weight}}
    </view>
  </view>


  <view class="weui-cells__title">
    <text>备注留言</text>
  </view>
  <view class="weui-cells weui-cells_form">
    <view class="weui-cell">
      <text class="weui-textarea">{{note}} </text>

    </view>
  </view>
</view>

<view style='height:40px'></view>


<view class='footer'>
  <view bindtap='shouye' class='rel'>
    <view class='icon rel'>
      <image mode='widthFix' class='ab udlr' src='/page/component/resources/pic/1.png'></image>
    </view>
    <view class='nav_title center'>首页</view>
    <form class='plate_form' bindsubmit="formid_one" report-submit="true" bindreset="formReset">
      <button form-type='submit'></button>
    </form>
  </view>
  <view id="{{item.id}}" class='nav_phone rel' style='background-color:#FF7F00'>
    <view class='icon rel'>
      <image mode='widthFix' class='ab udlr' src='/page/common/images/fen.png'></image>
    </view>
    <view style='color:white'> 分享好友</view>
    <form class='plate_form' bindsubmit="formid_one" report-submit="true" bindreset="formReset">
      <button form-type='submit' open-type="share"></button>
    </form>
  </view>
  <view bindtap='phone' class='nav_phone rel'>
    <view>一键拨号</view>
    <view> {{userphone}}</view>
    <form class='plate_form' bindsubmit="formid_one" report-submit="true" bindreset="formReset">
      <button form-type='submit'></button>
    </form>
  </view>
</view>