
.list-bd{ overflow: hidden; }
.list-bd .item{ margin: 0.5rem 0.625rem 0.5rem 0.625rem; padding: 1rem 1.625rem 0.625rem 1.625rem; 
background-color:#fff; border-radius: 10px; }

.list-operations{ margin: 10px 0 0 0; padding: 10px 0 0 0; overflow: hidden; }
.list-operations .btn-item{ height: 1.625rem; font-size:14px; line-height: 1.625rem; 
margin: 0 0 0 0.625rem; float:right; }
.list-operations .btn-item.edit{ color:#26B83E; background-color: rgba(38,184,62,.05); transition:all 0.2s; }
.list-operations .btn-item.edit:after{ border-color:rgba(38,184,62,1); }
.list-operations .btn-item.edit.button-hover{ background-color: rgba(0,0,0,.05); }
.banner{ height: 187.5rpx; }
.banner-img{ width:100%; height: 100%; }

.notice-text{ margin: 2px 0 0 5px}
.notice image{
  width: 17px;
  height: 17px;
}
.broadcast{ background-color: #fff; border-bottom-left-radius:10px; border-bottom-right-radius:10px; }

.broadcast-title{ overflow:hidden; font-size: 0.75rem; line-height:2rem; white-space: nowrap; text-overflow: ellipsis; margin: 0 0 0 0; padding: 0 0 0 1.625rem; color: #FF7F00; }
.container {height: 100%;display: flex;flex-direction: column;justify-content: space-between;box-sizing: border-box;}
.scrolltxt{padding:0 20rpx;background:white;}
.marquee_box {position:relative;color:#333;height:90rpx;display:block;overflow:hidden;} 
.marquee_text {white-space: nowrap;position:absolute;top:0;font-size:14px;height:90rpx;line-height:90rpx;}



.broadcast-icon{ width:3.75rem; height:2rem; position: relative; background: url(data:image/png;base64,) no-repeat center center; 
background-size: 3.0625rem 1.53125rem; float:left; }
.broadcast-title{ overflow:hidden; font-size: 0.75rem; line-height:2rem; white-space: nowrap; text-overflow: ellipsis; }